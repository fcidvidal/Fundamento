package arreglo_tres;
import java.util.Scanner;
/**
 *
 * @author dci
 */
public class Arreglo_tres {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        Scanner gl = new Scanner(System.in);
        int asign, nota, alumno;
        
       System.out.println("Cantidad de asignaturas");
       asign = gl.nextInt();
       
       System.out.println("Cantidad de alumno");
       alumno = gl.nextInt();
       
        System.out.println("Ingrese nota");
        nota = gl.nextInt();
       int [][][]matriz = new int [asign][alumno][nota];
    }
    
}
